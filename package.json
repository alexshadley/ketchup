{
  "scripts": {
    "start": "yarn concurrently \"yarn dev-frontend\" \"yarn dev-backend\"",
    "prod-start": "python app.py --reset-db --test-populate; yarn concurrently \"yarn prod-frontend\" \"yarn prod-backend\";",
    "build": "parcel build src/index.html",
    "prod-frontend": "yarn node frontend.js",
    "dev-frontend": "yarn concurrently \"yarn node frontend.js\" \"yarn parcel watch src/index.html --port 12345\"",
    "prod-backend": "gunicorn app:app --bind=0.0.0.0:4321",
    "dev-backend": "python app.py --start-server --reset-db --test-populate"
  },
  "name": "ketchup-main",
  "description": "a really cool app",
  "version": "1.0.0",
  "engines": {
    "yarn": "1.22"
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "concurrently": "^7.0.0",
    "csstype": "^2.6.2",
    "express": "^4.17.3",
    "googleapis": "^92.0.0",
    "graphql": "^16.3.0",
    "http-proxy-middleware": "^2.0.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-bootstrap-icons": "^1.7.2",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-hot-toast": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "parcel": "^2.2.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
